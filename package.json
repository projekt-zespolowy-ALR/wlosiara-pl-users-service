		"@types/jest": "^29.5.0",
		"@types/node": "^20.2.1",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.6",
		"dotenv": "^16.0.3",
		"envalid": "^7.3.1",
